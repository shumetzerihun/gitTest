#include<iostream>
#include<iomanip>
#include<vector>
using namespace std;

class Class1{
public:
	int staticVar;
	Class1(){
		++staticVar;
	}
	~Class1(){
		--staticVar;
	}

};

Class1::staticVar = 0;

int main(void) {

	Class1

	return 0;
}
