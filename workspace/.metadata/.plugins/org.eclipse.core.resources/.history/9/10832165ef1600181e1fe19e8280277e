#include <string>
#include <iostream>
#include <locale>
using namespace std;

string stack[2049] = {0};
int sp = 0;

void push(string value){
	stack[sp++] = value;
	//cout<<stack[--sp];
}

char pop(){
	return stack[--sp];
}

int main(void) {
	string expression = {"+13"}; //cout <<expression.size() - 1<<endl;
	int opNew = 0;

	for (int i = expression.size() - 1; i >= 0; i--){
		if (isdigit(expression[i])){
			//cout<<expression[i];
			push (expression[i]);
		}

		else{
			int op1 = pop() - '0'; //cout<<op1<<" ";
			int op2 = pop() - '0'; //cout<<op2<<" ";

			switch (expression[i]){
					case '+':
						string result = to_string(op1 + op2);
						push(operand);

						break;
					case '-':
						string result = to_string(op1 - op2);
						push(operand);
						break;
					case '*':
						string result = to_string(op1 * op2);
						push(operand);
						break;
					}
		}
	}

	for (int i = 0; i < 3; i++)
		cout<<pop();



	return 0;

}

