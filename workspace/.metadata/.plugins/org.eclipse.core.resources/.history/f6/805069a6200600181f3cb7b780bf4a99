#ifndef BIRTHDAY_H_
#define BIRTHDAY_H_

class Birthday{
private:
	int day;
	int month;
	int year;
public:
	Birthday(int d, int m, int y);
	~Birthday();
	printBirthday(int day, int month, int year);
};



#endif /* BIRTHDAY_H_ */
