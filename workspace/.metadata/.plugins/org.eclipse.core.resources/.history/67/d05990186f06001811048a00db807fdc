#include <iostream>
#include <string>

using namespace std;


class FriendClass{

private:
	int age;
public:
	FriendClass(int = 0);
	~FriendClass();

friend void printAge(friendClass a);

};


void printAge(friendClass x){

	cout<<x.age;
}

int main(){

	FriendClass aa;

	return 0;
}


