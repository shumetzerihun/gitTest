#include <iostream>
#include <fstream>
using namespace std;

int main(){

	/*int id;
	string name;
	double money;*/
	char ch;

	ifstream theFile("myFile.txt");

	while (theFile>>ch){
		//cout<<id<<"           "<<name<<"           "<<money<<endl;
		cout<<ch;
	}

	theFile.close();

	return 0;
}
