#include <iostream>
#include <string>
using namespace std;

void prinInfo(string theString){
	cout <<"Length: "<<theString.length()<<endl;
	cout <<"Capacity: "<<theString.capacity()<<endl;
	cout <<"Max size: "<<theString.max_size()<<endl<<endl;

}


