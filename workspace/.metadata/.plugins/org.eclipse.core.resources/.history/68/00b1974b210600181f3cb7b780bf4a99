#include <iostream>
#include <string>
#include "people.h"

using namespace std;

People::People(string name, Birthday bd): name(name), bd(bd){
}
People::~People(){}

void People::printPeople(string name, Birthday bd){
	cout<<"name: "<<name<<"Birthday: "<<bd<<endl;
}

