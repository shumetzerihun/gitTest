#include <iostream>
#include<iomanip>
#include <string>
using namespace std;


int main(void) {

	string expression;
	do{
		getline(cin, expression);
	}while(expression.length() > 0);

	while(expression.length() > 0){
		cout<<expression<<endl;
	}

	return 0;
}
