#include <iostream>
#include <string>

using namespace std;


class FriendClass{

private:
	int age;
public:
	FriendClass(int = 0);
	~FriendClass();

friend void printAge(FriendClass a);
};


void printAge(FriendClass x){

	cout<<x.age;
}

int main(){

	FriendClass bb;


	return 0;
}


